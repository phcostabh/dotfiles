#!/bin/bash

export EDITOR="gvim"
export PATH=$PATH:~/bin

###############################
# NodeJs
###############################

# NVM
export NVM_HOME="$HOME/.nvm"
[[ -f "$NVM_HOME/nvm.sh" ]] && source "$NVM_HOME/nvm.sh"
[[ -f "$NVM_HOME/bash_completion" ]] && source "$NVM_HOME/bash_completion" # NVM bash completion
